Функция ВернутьНомерБезСуффикса(Ном, Суффикс = "")
	Стр = "";
	Суффикс = "";
	Ном = СокрЛП(Ном);
	Для Сч = 1 По СтрДлина(Ном) Цикл
		Смв = Сред(Ном, Сч, 1);
		Если Смв = "_" Тогда
			НомСф = Сч + 1;
			Суффикс = Сред(Ном, НомСф, СтрДлина(Ном) - НомСф + 1);
			Прервать;
		КонецеСли;
		Стр = Стр + Смв;
	КонецЦикла;	
	Возврат Стр;
КонецФункции	


