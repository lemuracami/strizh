	////|	новаТранспорт.СтартНеНаСкладе КАК start_at,
	////|	новаТранспорт.ЗавершениеНеНаСкладе КАК finish_at,
	//|	""driving"" КАК routing_mode,
	//|	ИСТИНА КАК visit_depot_at_start,
