			<dataPath>ДатаДоставкиАдминка</dataPath>
			<field>ДатаДоставкиАдминка</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	dbo_mv_orders.orderIDStr КАК НомерЗаказаАдминки,
	ВЫБОР
		КОГДА dbo_mv_orders.payType = 1
				ИЛИ dbo_mv_orders.payType = 5
			ТОГДА ВЫБОР
					КОГДА dbo_mv_orders.status &lt;&gt; 3
						ТОГДА 0
					ИНАЧЕ dbo_mv_orders.orderSumRUR_all
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК СуммаАдминки,
	НАЧАЛОПЕРИОДА(dbo_mv_orders.completeDate, ДЕНЬ) КАК ДатаДоставкиАдминка
ИЗ
	ВнешнийИсточникДанных.ИнтернетМагазин.Таблица.dbo_mv_orders КАК dbo_mv_orders</query>
