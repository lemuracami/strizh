		<field xsi:type="DataSetFieldField">
			<dataPath>Отказы</dataPath>
			<field>Отказы</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Отказы</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ТерминалДоставки</dataPath>
			<field>ТерминалДоставки</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	новаОтчетВодителяЗадания.Ссылка.Транспорт КАК Транспорт,
	новаОтчетВодителяЗадания.Ссылка.Водитель КАК Водитель,
	новаОтчетВодителяЗадания.Ссылка.Экспедитор КАК Экспедитор,
	новаОтчетВодителяЗадания.Ссылка.Транспорт.НомерГосударственнойРегистрации КАК НомерАвто,
	ВЫБОР
		КОГДА новаОтчетВодителяЗадания.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.Выполнена)
				ИЛИ новаОтчетВодителяЗадания.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.ВыполненаЧастично)
				ИЛИ новаОтчетВодителяЗадания.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.НеВыполнена)
					И (новаОтчетВодителяЗадания.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ОтказКлиентаСЗаездом)
						ИЛИ новаОтчетВодителяЗадания.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ПереносСЗаездом))
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ДоставокВыполнено,
	ВЫБОР
		КОГДА новаОтчетВодителяЗадания.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.НеВыполнена)
				И (новаОтчетВодителяЗадания.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ОтказКлиентаБезЗаезда)
					ИЛИ новаОтчетВодителяЗадания.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ПереносДоставки))
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ДоставокНеВыполнено,
	NULL КАК ЗаборовВыполнено,
	NULL КАК ЗаборовНеВыполнено,
	ВЫБОР
		КОГДА новаОтчетВодителяЗадания.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ОтказКлиентаБезЗаезда)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК Отказы,
	ВЫБОР
		КОГДА новаОтчетВодителяЗадания.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ПереносДоставки)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК Переносы,
	новаОтчетВодителяЗадания.Задание КАК Задание,
	NULL КАК Забор,
	новаОтчетВодителяЗадания.Ссылка.ТерминалДоставки КАК ТерминалДоставки
ПОМЕСТИТЬ ВТ
ИЗ
	Документ.новаОтчетВодителя.Задания КАК новаОтчетВодителяЗадания
ГДЕ
	новаОтчетВодителяЗадания.Ссылка.Рейс.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	новаОтчетВодителяЗаборы.Ссылка.Транспорт,
	новаОтчетВодителяЗаборы.Ссылка.Водитель,
	новаОтчетВодителяЗаборы.Ссылка.Экспедитор,
	новаОтчетВодителяЗаборы.Ссылка.Транспорт.НомерГосударственнойРегистрации,
	NULL,
	NULL,
	ВЫБОР
		КОГДА новаОтчетВодителяЗаборы.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.Выполнена)
				ИЛИ новаОтчетВодителяЗаборы.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.ВыполненаЧастично)
				ИЛИ новаОтчетВодителяЗаборы.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.НеВыполнена)
					И новаОтчетВодителяЗаборы.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ОтказКлиентаСЗаездом)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА новаОтчетВодителяЗаборы.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.НеВыполнена)
				И НЕ новаОтчетВодителяЗаборы.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ОтказКлиентаСЗаездом)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА новаОтчетВодителяЗаборы.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ОтказКлиентаБезЗаезда)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА новаОтчетВодителяЗаборы.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ПереносДоставки)
				ИЛИ новаОтчетВодителяЗаборы.ПричинаНевыполнения = ЗНАЧЕНИЕ(Справочник.ПричиныНеВыполненияДоставки.ПереносСЗаездом)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ,
	NULL,
	новаОтчетВодителяЗаборы.Забор,
	новаОтчетВодителяЗаборы.Ссылка.ТерминалДоставки
ИЗ
	Документ.новаОтчетВодителя.Заборы КАК новаОтчетВодителяЗаборы
ГДЕ
	новаОтчетВодителяЗаборы.Ссылка.Рейс.Дата МЕЖДУ &amp;Дата1 И &amp;Дата2
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ.Транспорт КАК Транспорт,
	ВТ.Водитель КАК Водитель,
	ВТ.Экспедитор КАК Экспедитор,
	ВТ.НомерАвто КАК НомерАвто,
	СУММА(ВТ.ДоставокВыполнено) КАК ДоставокВыполнено,
	СУММА(ВТ.ДоставокНеВыполнено) КАК ДоставокНеВыполнено,
	СУММА(ВТ.ЗаборовВыполнено) КАК ЗаборовВыполнено,
	СУММА(ВТ.ЗаборовНеВыполнено) КАК ЗаборовНеВыполнено,
	СУММА(ВТ.Отказы) КАК Отказы,
	СУММА(ВТ.Переносы) КАК Переносы,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ВТ.Задание) КАК ВсегоЗаказов,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ВТ.Забор) КАК ВсегоЗаборов,
	ВТ.ТерминалДоставки КАК ТерминалДоставки
ПОМЕСТИТЬ ВТ_Итог
ИЗ
	ВТ КАК ВТ

СГРУППИРОВАТЬ ПО
	ВТ.НомерАвто,
	ВТ.Водитель,
	ВТ.Транспорт,
	ВТ.Экспедитор,
	ВТ.ТерминалДоставки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_Итог.Транспорт КАК Транспорт,
	ВТ_Итог.Водитель КАК Водитель,
	ВТ_Итог.Экспедитор КАК Экспедитор,
	ВТ_Итог.НомерАвто КАК НомерАвто,
	ВТ_Итог.ДоставокВыполнено КАК ДоставокВыполнено,
	ВТ_Итог.ДоставокНеВыполнено КАК ДоставокНеВыполнено,
	ВТ_Итог.ЗаборовВыполнено КАК ЗаборовВыполнено,
	ВТ_Итог.ЗаборовНеВыполнено КАК ЗаборовНеВыполнено,
	ЕСТЬNULL(ВТ_Итог.ВсегоЗаказов, 0) + ЕСТЬNULL(ВТ_Итог.ВсегоЗаборов, 0) КАК Всего,
	ВТ_Итог.ВсегоЗаказов КАК ВсегоЗаказов,
	ВТ_Итог.ВсегоЗаборов КАК ВсегоЗаборов,
	ЕСТЬNULL(ВТ_Итог.ДоставокВыполнено, 0) + ЕСТЬNULL(ВТ_Итог.ЗаборовВыполнено, 0) КАК ВсегоВыполнено,
	ВТ_Итог.Отказы КАК Отказы,
	ВТ_Итог.Переносы КАК Переносы,
	ВТ_Итог.ТерминалДоставки КАК ТерминалДоставки
ИЗ
	ВТ_Итог КАК ВТ_Итог
ГДЕ
	ВТ_Итог.ТерминалДоставки В(&amp;ТерминалПриема)</query>
	</dataSet>
	<calculatedField>
		<dataPath>Выполнение</dataPath>
		<expression>ВсегоВыполнено/Всего * 100</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Выполнение</v8:content>
			</v8:item>
		</title>
		<appearance>
			<dcscor:item xsi:type="dcsset:SettingsParameterValue">
				<dcscor:parameter>Формат</dcscor:parameter>
				<dcscor:value xsi:type="xs:string">ЧДЦ=2; ЧН=0</dcscor:value>
			</dcscor:item>
		</appearance>
	</calculatedField>
	<totalField>
		<dataPath>Всего</dataPath>
		<expression>Сумма(Всего)</expression>
	</totalField>
	<totalField>
		<dataPath>ВсегоВыполнено</dataPath>
		<expression>Сумма(ВсегоВыполнено)</expression>
	</totalField>
	<totalField>
		<dataPath>ВсегоЗаборов</dataPath>
		<expression>Сумма(ВсегоЗаборов)</expression>
	</totalField>
	<totalField>
		<dataPath>ВсегоЗаказов</dataPath>
		<expression>Сумма(ВсегоЗаказов)</expression>
	</totalField>
	<totalField>
		<dataPath>Выполнение</dataPath>
		<expression>ВЫЧИСЛИТЬ("Сумма(ВсегоВыполнено)/Сумма(Всего)* 100")</expression>
		<group>Выполнение</group>
		<group>ТерминалДоставки</group>
		<group>ОбщийИтог</group>
	</totalField>
	<totalField>
		<dataPath>ДоставокВыполнено</dataPath>
		<expression>Сумма(ДоставокВыполнено)</expression>
	</totalField>
	<totalField>
		<dataPath>ДоставокНеВыполнено</dataPath>
		<expression>Сумма(ДоставокНеВыполнено)</expression>
	</totalField>
	<totalField>
		<dataPath>ЗаборовВыполнено</dataPath>
		<expression>Сумма(ЗаборовВыполнено)</expression>
	</totalField>
	<totalField>
		<dataPath>ЗаборовНеВыполнено</dataPath>
		<expression>Сумма(ЗаборовНеВыполнено)</expression>
	</totalField>
	<totalField>
		<dataPath>Отказы</dataPath>
		<expression>Сумма(Отказы)</expression>
	</totalField>
	<totalField>
		<dataPath>Переносы</dataPath>
		<expression>Сумма(Переносы)</expression>
	</totalField>
