			<field>НачалоРейса</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>РасстояниеПлан</dataPath>
			<field>РасстояниеПлан</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>РасстояниеФакт</dataPath>
			<field>РасстояниеФакт</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Рейс</dataPath>
			<field>Рейс</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ТочкаМаршрута</dataPath>
			<field>ТочкаМаршрута</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Транспорт</dataPath>
			<field>Транспорт</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Экспедитор</dataPath>
			<field>Экспедитор</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаПрибытияПлан</dataPath>
			<field>ДатаПрибытияПлан</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаПрибытияФакт</dataPath>
			<field>ДатаПрибытияФакт</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаУбытияПдан</dataPath>
			<field>ДатаУбытияПдан</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаУбытияФакт</dataPath>
			<field>ДатаУбытияФакт</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Вес</dataPath>
			<field>Вес</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВремяПрибытияПо</dataPath>
			<field>ВремяПрибытияПо</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВремяПрибытияС</dataPath>
			<field>ВремяПрибытияС</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Объем</dataPath>
			<field>Объем</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Палетт</dataPath>
