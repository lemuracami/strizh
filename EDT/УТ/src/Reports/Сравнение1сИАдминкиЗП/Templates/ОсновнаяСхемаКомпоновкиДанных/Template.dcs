	<dataSet xsi:type="DataSetQuery">
		<name>НаборАдминка</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>typeDelivery</dataPath>
			<field>typeDelivery</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>МашинаВАдминке</dataPath>
			<field>МашинаВАдминке</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Машина в админке</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>orderIDStr</dataPath>
			<field>orderIDStr</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	dbo_mv_orders.carNumber КАК МашинаВАдминке,
	dbo_mv_orders.typeDelivery,
	dbo_mv_orders.orderIDStr КАК orderIDStr
ИЗ
	ВнешнийИсточникДанных.ИнтернетМагазин.Таблица.dbo_mv_orders КАК dbo_mv_orders
ГДЕ
	dbo_mv_orders.status = 3
	И dbo_mv_orders.deliveryDate МЕЖДУ &amp;ДатаНач И &amp;ДатаКон</query>
	</dataSet>
