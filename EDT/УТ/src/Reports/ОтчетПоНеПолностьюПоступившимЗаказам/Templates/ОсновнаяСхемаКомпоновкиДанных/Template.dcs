			<field>ТерминалОбработки</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаСтатусаМеста</dataPath>
			<field>ДатаСтатусаМеста</field>
			<title xsi:type="v8:LocalStringType">
