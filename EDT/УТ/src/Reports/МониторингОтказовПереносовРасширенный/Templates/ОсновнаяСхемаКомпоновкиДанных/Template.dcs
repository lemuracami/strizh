		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ВЫРАЗИТЬ(СостоянияЗаказов.Регистратор КАК Документ.новаОтчетВодителя).Рейс.Дата КАК Период,
	СостоянияЗаказов.Доставка КАК Доставка,
	СостоянияЗаказов.Заказ КАК Заказ,
	СостоянияЗаказов.РезультатДоставки КАК РезультатДоставки,
	СостоянияЗаказов.ПричинаНеВыполнения КАК ПричинаНеВыполнения,
	СостоянияЗаказов.ПричинаОтказа КАК ПричинаОтказа,
	СостоянияЗаказов.ДатаПереноса КАК ДатаПереноса,
	СостоянияЗаказов.Заказ.Номер КАК ЗаказНомер,
	СостоянияЗаказов.Регистратор КАК Регистратор,
	СостоянияЗаказов.Регистратор.Водитель КАК Водитель,
	СостоянияЗаказов.Регистратор.Транспорт КАК Транспорт,
	СостоянияЗаказов.Регистратор.Экспедитор КАК Экспедитор,
	ЕСТЬNULL(ОперативноеНазначениеТАНаРейсСрезПоследних.ТА, ТелефонныеАппаратыТранспортаСрезПоследних.ТА.Телефон) КАК Телефон,
	СостоянияЗаказов.Заказ.Телефон КАК ТелефонКлиента,
	СостоянияЗаказов.Заказ.ВладелецТовара КАК Магазин,
	СостоянияЗаказов.Доставка.ВремяПрибытияПо КАК ВременнойИнтервал,
	СостоянияЗаказов.Доставка.ВремяПрибытияС КАК ДоставкаВремяПрибытияС,
	СостоянияЗаказов.Доставка.ВремяПрибытияПо КАК ДоставкаВремяПрибытияПо,
	ЕСТЬNULL(ПараметрыТарифаЗаказаСрезПоследних.ЗонаТарификации, ЗНАЧЕНИЕ(Справочник.ГруппыРайонов.ПустаяСсылка)) КАК ЗаказЗонаТарификации,
	ВЫБОР
		КОГДА СостоянияЗаказов.Регистратор.Экспедитор = ЗНАЧЕНИЕ(Справочник.новаЭкспедиторы.ПустаяСсылка)
			ТОГДА СостоянияЗаказов.Регистратор.Водитель.ДатаПриемаНаРаботу
		ИНАЧЕ СостоянияЗаказов.Регистратор.Экспедитор.ДатаПриемаНаРаботу
	КОНЕЦ КАК ДатаПриемаНаРаботу
ПОМЕСТИТЬ ВТ
ИЗ
	РегистрСведений.СостоянияЗаказов КАК СостоянияЗаказов
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ТелефонныеАппаратыТранспорта.СрезПоследних КАК ТелефонныеАппаратыТранспортаСрезПоследних
		ПО СостоянияЗаказов.Регистратор.Транспорт = ТелефонныеАппаратыТранспортаСрезПоследних.Транспорт
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ОперативноеНазначениеТАНаРейс.СрезПоследних КАК ОперативноеНазначениеТАНаРейсСрезПоследних
		ПО СостоянияЗаказов.Регистратор.Рейс = ОперативноеНазначениеТАНаРейсСрезПоследних.Рейс.РейсМестнойДоставки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПараметрыТарифаЗаказа.СрезПоследних({(&amp;ПериодСреза)}, ) КАК ПараметрыТарифаЗаказаСрезПоследних
		ПО СостоянияЗаказов.Заказ = ПараметрыТарифаЗаказаСрезПоследних.Заказ
ГДЕ
	СостоянияЗаказов.Регистратор ССЫЛКА Документ.новаОтчетВодителя
	И СостоянияЗаказов.РезультатДоставки = ЗНАЧЕНИЕ(Справочник.новаРезультатМестнойДоставки.НеВыполнена)
	И ВЫРАЗИТЬ(СостоянияЗаказов.Регистратор КАК Документ.новаОтчетВодителя).Рейс.Дата МЕЖДУ &amp;Период1 И &amp;Период2
	И СостоянияЗаказов.Заказ.ТерминалДоставки = &amp;Терминал
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДанныеЗаказаВодителя.Реализация КАК Реализация,
	МАКСИМУМ(ДанныеЗаказаВодителя.Дата) КАК Дата,
	ДанныеЗаказаВодителя.ПричинаНеВыполненияДоставки КАК ПричинаНеВыполненияДоставки,
	ДанныеЗаказаВодителя.ПричинаОтказаПереноса КАК ПричинаОтказаПереноса,
	ДанныеЗаказаВодителя.Водитель КАК Водитель
ПОМЕСТИТЬ ВТ_ДанныеВодителя
ИЗ
	Документ.ДанныеЗаказаВодителя КАК ДанныеЗаказаВодителя
ГДЕ
	ДанныеЗаказаВодителя.Реализация В
			(ВЫБРАТЬ
				ВТ.Заказ
			ИЗ
				ВТ КАК ВТ)
	И ДанныеЗаказаВодителя.Дата МЕЖДУ &amp;Период1 И &amp;Период2

СГРУППИРОВАТЬ ПО
	ДанныеЗаказаВодителя.Реализация,
	ДанныеЗаказаВодителя.Водитель,
	ДанныеЗаказаВодителя.ПричинаОтказаПереноса,
	ДанныеЗаказаВодителя.ПричинаНеВыполненияДоставки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ.Период КАК Период,
	ВТ.Доставка КАК Доставка,
	ВТ.Заказ КАК Заказ,
	ВТ.РезультатДоставки КАК РезультатДоставки,
	ВТ.ПричинаНеВыполнения КАК ПричинаНеВыполнения,
	ВТ.ПричинаОтказа КАК ПричинаОтказа,
	ВТ.ДатаПереноса КАК ДатаПереноса,
	ВТ.ЗаказНомер КАК ЗаказНомер,
	ВТ.Регистратор КАК Регистратор,
	ВТ.Водитель КАК Водитель,
	ВТ.Транспорт КАК Транспорт,
	ВТ.Экспедитор КАК Экспедитор,
	ВТ.Телефон КАК Телефон,
	ВТ.ТелефонКлиента КАК ТелефонКлиента,
	ЕСТЬNULL(ВТ_ДанныеВодителя.Дата, ВТ.Период) КАК ВремяПроставления,
	ВТ.Магазин КАК Магазин,
	ВТ.ВременнойИнтервал КАК ВременнойИнтервал,
	ВТ.ДоставкаВремяПрибытияС КАК ВремяС,
	ВТ.ДоставкаВремяПрибытияПо КАК ВремяПо,
	ВТ.ЗаказЗонаТарификации КАК ЗонаТарификации,
	ВЫБОР
		КОГДА ВТ.ДатаПриемаНаРаботу = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
				ИЛИ ВТ.ДатаПриемаНаРаботу ЕСТЬ NULL
			ТОГДА ""
		КОГДА РАЗНОСТЬДАТ(ВТ.ДатаПриемаНаРаботу, &amp;ТекущаяДата, ДЕНЬ) &gt;= 365
			ТОГДА ВЫРАЗИТЬ(РАЗНОСТЬДАТ(ВТ.ДатаПриемаНаРаботу, &amp;ТекущаяДата, ДЕНЬ) / 365 КАК ЧИСЛО(3, 1))
		ИНАЧЕ ВЫРАЗИТЬ(РАЗНОСТЬДАТ(ВТ.ДатаПриемаНаРаботу, &amp;ТекущаяДата, ДЕНЬ) / 30 КАК ЧИСЛО(2, 0))
	КОНЕЦ КАК ЗначениеСтажа,
	ВЫБОР
		КОГДА ВТ.ДатаПриемаНаРаботу = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
				ИЛИ ВТ.ДатаПриемаНаРаботу ЕСТЬ NULL
			ТОГДА ""
		КОГДА РАЗНОСТЬДАТ(ВТ.ДатаПриемаНаРаботу, &amp;ТекущаяДата, ДЕНЬ) &gt;= 365
			ТОГДА " год."
		ИНАЧЕ " мес."
	КОНЕЦ КАК ПериодичностьСтажа
ИЗ
	ВТ КАК ВТ
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ДанныеВодителя КАК ВТ_ДанныеВодителя
		ПО ВТ.Заказ = ВТ_ДанныеВодителя.Реализация
			И ВТ.ПричинаНеВыполнения = ВТ_ДанныеВодителя.ПричинаНеВыполненияДоставки
			И ВТ.ПричинаОтказа = ВТ_ДанныеВодителя.ПричинаОтказаПереноса
			И (ВТ_ДанныеВодителя.Дата МЕЖДУ НАЧАЛОПЕРИОДА(ВТ.Период, ДЕНЬ) И КОНЕЦПЕРИОДА(ВТ.Период, ДЕНЬ))</query>
	</dataSet>
	<calculatedField>
		<dataPath>Стаж</dataPath>
		<expression>Строка(ЗначениеСтажа) + ПериодичностьСтажа</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Стаж</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<parameter>
		<name>Период1</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Период1</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
