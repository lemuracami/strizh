    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>Список.Ссылка</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	упрЗаписьРасходов.Дата КАК Дата,
	упрЗаписьРасходов.Номер КАК Номер,
	упрЗаписьРасходов.Регион КАК Регион,
	упрЗаписьРасходов.Контрагент КАК Контрагент,
	упрЗаписьРасходов.СуммаПлатежа КАК СуммаПлатежа,
	упрЗаписьРасходов.СуммаПлатежа - ЕСТЬNULL(ВЗ_Оплачено.СуммаОплачено, 0) КАК Остаток,
	упрЗаписьРасходов.СтавкаНДС КАК СтавкаНДС,
	ВЫРАЗИТЬ(упрЗаписьРасходов.НазначениеПлатежа КАК СТРОКА(500)) КАК НазначениеПлатежа,
	упрЗаписьРасходов.Комментарий КАК Комментарий,
	упрЗаписьРасходов.НомерВходящегоСчета КАК НомерВходящегоСчета,
	упрЗаписьРасходов.ДатаВходящегоСчета КАК ДатаВходящегоСчета,
	упрЗаписьРасходов.СрокОплаты КАК СрокОплаты,
	ЕСТЬNULL(упрСостоянияЗаписейРасходовСрезПоследних.СостояниеЗаписиРасходов, ЗНАЧЕНИЕ(Перечисление.упрСостоянияЗаписейРасходов.Новый)) КАК Состояние,
	упрЗаписьРасходов.СтатьяРасходов КАК СтатьяРасходов,
	упрЗаписьРасходов.СуммаКУчету КАК СуммаКУчету,
	упрЗаписьРасходов.ВидПериодаУчета КАК ВидПериодаУчета,
	упрЗаписьРасходов.НачалоПериодаУчета КАК НачалоПериодаУчета,
	упрЗаписьРасходов.ОкончаниеПериодаУчета КАК ОкончаниеПериодаУчета,
	ВЫРАЗИТЬ(&quot;&quot; КАК СТРОКА(30)) КАК ПериодУчета,
	упрЗаписьРасходов.Ссылка КАК Ссылка,
	ВЫБОР
		КОГДА упрЗаписьРасходов.Проведен
			ТОГДА 0
		КОГДА упрЗаписьРасходов.ПометкаУдаления
			ТОГДА 1
		ИНАЧЕ 2
	КОНЕЦ КАК Картинка,
	ВЫБОР
		КОГДА ВЗ_Оплачено.СуммаОплачено ЕСТЬ NULL
			ТОГДА -1
		КОГДА упрЗаписьРасходов.СуммаПлатежа - ЕСТЬNULL(ВЗ_Оплачено.СуммаОплачено, 0) = 0
			ТОГДА 3
		ИНАЧЕ 1
	КОНЕЦ КАК ПлатежноеПоручение
ИЗ
	Документ.упрЗаписьРасходов КАК упрЗаписьРасходов
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.упрСостоянияЗаписейРасходов.СрезПоследних КАК упрСостоянияЗаписейРасходовСрезПоследних
		ПО упрЗаписьРасходов.Ссылка = упрСостоянияЗаписейРасходовСрезПоследних.ЗаписьРасходов
		ЛЕВОЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			упрЗаписьРасходов.Ссылка КАК ЗаписьРасхода,
			СУММА(ВЫБОР
					КОГДА упрСостоянияПлатежейСрезПоследних.СостояниеПлатежа = ЗНАЧЕНИЕ(Перечисление.упрСостоянияПлатежей.Оплачено)
						ТОГДА упрСостоянияПлатежейСрезПоследних.Платеж.СуммаПлатежа
					ИНАЧЕ 0
				КОНЕЦ) КАК СуммаОплачено
		ИЗ
			Документ.упрЗаписьРасходов КАК упрЗаписьРасходов
				ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.упрСостоянияПлатежей.СрезПоследних КАК упрСостоянияПлатежейСрезПоследних
				ПО (упрЗаписьРасходов.Дата МЕЖДУ &amp;НачалоПериода И &amp;ОкончаниеПериода)
					И (упрЗаписьРасходов.Организация = &amp;Организация)
					И упрЗаписьРасходов.Ссылка = упрСостоянияПлатежейСрезПоследних.Платеж.ЗаписьРасхода
					И (НЕ упрСостоянияПлатежейСрезПоследних.Платеж.ПометкаУдаления)
					И (упрСостоянияПлатежейСрезПоследних.СостояниеПлатежа В (ЗНАЧЕНИЕ(Перечисление.упрСостоянияПлатежей.Оплачено), ЗНАЧЕНИЕ(Перечисление.упрСостоянияПлатежей.Подготовлено)))
		{ГДЕ
			упрЗаписьРасходов.Регион.*}
		
		СГРУППИРОВАТЬ ПО
			упрЗаписьРасходов.Ссылка) КАК ВЗ_Оплачено
		ПО упрЗаписьРасходов.Ссылка = ВЗ_Оплачено.ЗаписьРасхода
ГДЕ
	упрЗаписьРасходов.Дата МЕЖДУ &amp;НачалоПериода И &amp;ОкончаниеПериода
	И упрЗаписьРасходов.Организация = &amp;Организация
{ГДЕ
	упрЗаписьРасходов.Регион.*,
	(ЕСТЬNULL(упрСостоянияЗаписейРасходовСрезПоследних.СостояниеЗаписиРасходов, ЗНАЧЕНИЕ(Перечисление.упрСостоянияЗаписейРасходов.Новый))).* КАК Состояние}

ОБЪЕДИНИТЬ

ВЫБРАТЬ
	упрЗаписьРасходов.Дата,
	упрЗаписьРасходов.Номер,
	упрЗаписьРасходов.Регион,
	упрЗаписьРасходов.Контрагент,
	упрЗаписьРасходов.СуммаПлатежа,
	упрЗаписьРасходов.СуммаПлатежа - ЕСТЬNULL(ВЗ_Оплачено.СуммаОплачено, 0),
	упрЗаписьРасходов.СтавкаНДС,
	ВЫРАЗИТЬ(упрЗаписьРасходов.НазначениеПлатежа КАК СТРОКА(500)),
	упрЗаписьРасходов.Комментарий,
	упрЗаписьРасходов.НомерВходящегоСчета,
	упрЗаписьРасходов.ДатаВходящегоСчета,
	упрЗаписьРасходов.СрокОплаты,
	упрСостоянияЗаписейРасходовСрезПоследних.СостояниеЗаписиРасходов,
	упрЗаписьРасходов.СтатьяРасходов,
	упрЗаписьРасходов.СуммаКУчету,
	упрЗаписьРасходов.ВидПериодаУчета,
	упрЗаписьРасходов.НачалоПериодаУчета,
	упрЗаписьРасходов.ОкончаниеПериодаУчета,
	ВЫРАЗИТЬ(&quot;&quot; КАК СТРОКА(30)),
	упрЗаписьРасходов.Ссылка,
	ВЫБОР
		КОГДА упрЗаписьРасходов.Проведен
			ТОГДА 0
		КОГДА упрЗаписьРасходов.ПометкаУдаления
			ТОГДА 1
		ИНАЧЕ 2
	КОНЕЦ,
	ВЫБОР
		КОГДА ВЗ_Оплачено.СуммаОплачено ЕСТЬ NULL
			ТОГДА -1
		КОГДА упрЗаписьРасходов.СуммаПлатежа - ЕСТЬNULL(ВЗ_Оплачено.СуммаОплачено, 0) = 0
			ТОГДА 3
		ИНАЧЕ 1
	КОНЕЦ
ИЗ
	Документ.упрЗаписьРасходов КАК упрЗаписьРасходов
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.упрСостоянияЗаписейРасходов.СрезПоследних КАК упрСостоянияЗаписейРасходовСрезПоследних
		ПО (упрСостоянияЗаписейРасходовСрезПоследних.СостояниеЗаписиРасходов = ЗНАЧЕНИЕ(Перечисление.упрСостоянияЗаписейРасходов.Новый))
			И упрЗаписьРасходов.Ссылка = упрСостоянияЗаписейРасходовСрезПоследних.ЗаписьРасходов
			И (упрЗаписьРасходов.Организация = &amp;Организация),
	(ВЫБРАТЬ
		упрЗаписьРасходов.Ссылка КАК ЗаписьРасхода,
		СУММА(ВЫБОР
				КОГДА упрСостоянияПлатежейСрезПоследних.СостояниеПлатежа = ЗНАЧЕНИЕ(Перечисление.упрСостоянияПлатежей.Оплачено)
					ТОГДА упрСостоянияПлатежейСрезПоследних.Платеж.СуммаПлатежа
				ИНАЧЕ 0
			КОНЕЦ) КАК СуммаОплачено
	ИЗ
		Документ.упрЗаписьРасходов КАК упрЗаписьРасходов
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.упрСостоянияЗаписейРасходов.СрезПоследних КАК упрСостоянияЗаписейРасходовСрезПоследних
			ПО (упрСостоянияЗаписейРасходовСрезПоследних.СостояниеЗаписиРасходов = ЗНАЧЕНИЕ(Перечисление.упрСостоянияЗаписейРасходов.Новый))
				И упрЗаписьРасходов.Ссылка = упрСостоянияЗаписейРасходовСрезПоследних.ЗаписьРасходов
				И (упрЗаписьРасходов.Организация = &amp;Организация)
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.упрСостоянияПлатежей.СрезПоследних КАК упрСостоянияПлатежейСрезПоследних
			ПО упрЗаписьРасходов.Ссылка = упрСостоянияПлатежейСрезПоследних.Платеж.ЗаписьРасхода
				И (НЕ упрСостоянияПлатежейСрезПоследних.Платеж.ПометкаУдаления)
				И (упрСостоянияПлатежейСрезПоследних.СостояниеПлатежа В (ЗНАЧЕНИЕ(Перечисление.упрСостоянияПлатежей.Оплачено), ЗНАЧЕНИЕ(Перечисление.упрСостоянияПлатежей.Подготовлено)))
	{ГДЕ
		упрЗаписьРасходов.Регион.*}
	
	СГРУППИРОВАТЬ ПО
		упрЗаписьРасходов.Ссылка) КАК ВЗ_Оплачено
{ГДЕ
	упрЗаписьРасходов.Регион.*,
	упрСостоянияЗаписейРасходовСрезПоследних.СостояниеЗаписиРасходов.* КАК Состояние}</queryText>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
      <keyField>Ссылка</keyField>
    </extInfo>
